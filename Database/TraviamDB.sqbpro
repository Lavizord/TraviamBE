                INNER JOIN TilesMapa
                    ON Edificios.TileID = TilesMapa.Id
                WHERE
                    Edificios.isBuilding = 0 AND
                    TilesMapa.ID = 67 AND
                    Edificios.Nome = 'Pedreira'

            ) as SumPedra, (
